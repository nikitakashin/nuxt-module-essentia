{
  "name": "nuxt-module-essentia",
  "version": "1.0.5",
  "description": "Nuxt module for Essentia WASM integration",
  "type": "module",
  "main": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "types": "./dist/module.d.ts"
    },
    "./runtime": {
      "import": "./src/runtime/index.ts",
      "types": "./src/runtime/index.ts"
    }
  },
  "files": [
    "dist",
    "src/runtime"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0",
    "nuxt": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikitakashin/nuxt-module-essentia.git"
  },
  "author": {
    "name": "Kashin Nikita",
    "url": "https://github.com/nikitakashin/",
    "email": "dev@nkashin.ru"
  },
  "published": "11.2025",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "essentia",
    "essentia-wasm",
    "audio-analysis",
    "music-information-retrieval",
    "wasm",
    "web-audio",
    "audio-processing",
    "machine-learning",
    "signal-processing",
    "feature-extraction",
    "nuxt3",
    "nuxt4"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikitakashin/nuxt-module-essentia/issues"
  },
  "homepage": "https://github.com/nikitakashin/nuxt-module-essentia"
}
